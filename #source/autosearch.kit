<!DOCTYPE html>
<html lang="en">

<head>
	<title>Home</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
	<link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css" />
	</link>
	<link href="https://cdnjs.cloudflare.com/ajax/libs/slim-select/1.27.1/slimselect.min.css" rel="stylesheet">
	</link>
	<link rel="stylesheet" href="../node_modules/nouislider/dist/nouislider.css">
	</link>
	<link rel="stylesheet" href="css/style.css">
</head>

<body>
	<div class="main-wrapper">
		<!-- @import "main-kit/header.kit" -->
		<div class="content-wrapper">
			<section class="autosearch">
				<div class="container">
					<div class="breadcrumbs">
						<a href="">Home</a>
						<a href="" class="cur-page">Comparison</a>
					</div>
					<div class="prew-wrapper">
						<div class="text-block">
							<h1>Autosearch</h1>
							<h4>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been
								the industry's.</h4>
						</div>
						<div class="img-wrapper">
							<img src="/img/contact-img.png" alt="contact-img">
						</div>
					</div>
					<div class="form-wrapper">
						<form action="">
							<div class="group select">
								<label for="category">Category *</label>
								<select class="slim-select" id="category">
									<option value="value 1">Value 1</option>
									<option value="value 2">Value 2</option>
									<option value="value 3">Value 333333333333 444444 5666666666</option>
								</select>
							</div>
							<div class="group select">
								<label for="single">Category</label>
								<select id="slim-select" class="slim-select">
									<option value="value 1">Value 1</option>
									<option value="value 2">Value 2</option>
									<option value="value 3">Value 333333333333 444444 5666666666</option>
								</select>
							</div>
							<div class="group select">
								<label for="brands">Brands *</label>
								<select class="slim-select" id="brands">
									<option value="value 1">Value 1</option>
									<option value="value 2">Value 2</option>
									<option value="value 3">Value 333333333333 444444 5666666666</option>
								</select>
							</div>
							<div class="group select">
								<label for="single">Fuel</label>
								<select class="slim-select">
									<option value="value 1">Value 1</option>
									<option value="value 2">Value 2</option>
									<option value="value 3">Value 333333333333 444444 5666666666</option>
								</select>
							</div>
							<div class="group range">
								<label for="">Year</label>
								<div class="value-wrapper">
									<input class="slider-value-min" type="text">
									<!-- <div class="group select">
										<select class="slim-select slider-value-min" id="year">
											<option value="value 1">1997</option>
											<option value="value 2">1998</option>
											<option value="value 3">1999</option>
										</select>
									</div> -->
									<span>-</span>
									<!-- <div class="group select">
										<select class="slim-select slider-value-max" id="year">
											<option value="1997">1997</option>
											<option value="1998">1998</option>
											<option value="1999">1999</option>
										</select>
									</div> -->
									<input class="slider-value-max" type="text">
								</div>
								<div class="range-slider js-range" data-noui-min="1997" data-noui-max="2022" data-noui-step="1">
								</div>
							</div>
							<div class="group range">
								<div class="group-wrapper">
									<label for="">Price from-to</label>
									<div class="price-changer">
										<button class="price active">SEK</button>
										<button class="price">EUR</button>
										<button class="price">USD</button>
									</div>
								</div>
								<div class="value-wrapper">
									<input class="slider-value-min" type="text">
									<span>-</span>
									<input class="slider-value-max" type="text">
								</div>
								<div class="range-slider js-range" data-noui-min="30000" data-noui-max="5000000"
									data-noui-step="100">
								</div>
							</div>
							<div class="group range">
								<label for="">Meter reading from-to</label>
								<div class="value-wrapper">
									<input class="slider-value-min" type="text">
									<span>-</span>
									<input class="slider-value-max" type="text">
								</div>
								<div class="range-slider js-range" data-noui-min="0" data-noui-max="2000000" data-noui-step="1">
								</div>
							</div>
							<div class="group range">
								<label for="">Length from-to</label>
								<div class="value-wrapper">
									<input class="slider-value-min" type="text">
									<span>-</span>
									<input class="slider-value-max" type="text">
								</div>
								<div class="range-slider js-range" data-noui-min="5" data-noui-max="25" data-noui-step="1">
								</div>
							</div>
							<div class="group range">
								<label for="">Seats from-to</label>
								<div class="value-wrapper">
									<input class="slider-value-min" type="text">
									<span>-</span>
									<input class="slider-value-max" type="text">
								</div>
								<div class="range-slider js-range" data-noui-min="10" data-noui-max="40" data-noui-step="1">
								</div>
							</div>
							<div class="group select">
								<label for="single">Environmental engine</label>
								<select class="slim-select">
									<option value="value 1">Value 1</option>
									<option value="value 2">Value 2</option>
									<option value="value 3">Value 333333333333 444444 5666666666</option>
								</select>
							</div>
							<div class="group control w-100">
								<label for="model">Email</label>
								<input id="model" type="mail" placeholder="Your email address">
							</div>
							<div class="group textarea w-100">
								<label for="search">Other</label>
								<textarea placeholder="Enter Message"></textarea>
							</div>
							<button class="btn blue">Send</button>
						</form>
					</div>
				</div>
			</section>
		</div>
		<!-- @import "main-kit/footer.kit" -->
	</div>
	<!-- @import "main-kit/js.kit" -->
</body>

</html>